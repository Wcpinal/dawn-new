
  <footer>
        <div class="container">
          
            <div class="footer_main">
              
              {% for block in section.blocks %}
                {% if block.type == "slide" %}
                <div class="footer_left">
                      
                        <figure>
                          {% if block.settings.logo != blank %}
                            <a href="#">
                                <img src="{{ block.settings.logo | img_url: 'original' }} " alt="">
                            </a>
                            {% endif %}
                        </figure>
                      
                    <div class="c_contact">
                      
                        <p>{{ block.settings.add }}
                        </p>

                        <a href="tel:(800) 236-1366" class="number">
                          {{ block.settings.pho }}
                        </a>

                        <span>
                            {{ block.settings.fex }}
                        </span>

                        <a href="mailto:sales@greatlakesbio.com" class="email">
                            {{ block.settings.email }}
                        </a>
                    </div>
                      
                  {% if block.settings.show_icons != true %}
                    <div class="social_icons" style="display:none">
                        <ul>
                            <li>
                                <a href="{{ settings.social_facebook_link }}">
                                     {%- render 'facebook icon'| escape  -%}
                                </a>
                            </li>
                            <li>
                                <a href="{{ settings.social_twitter_link }}">
                                     {%- render 'twitter-icon'| escape  -%}
                                </a>
                            </li>
                            <li>
                               <a href="{{ settings.social_youtube_link }}">
                                     {%- render 'youtube-icon'| escape  -%}
                                </a>
                            </li>
                            <li>
                                <a href="{{ settings.social_linkdin_link }}">
                                   {%- render 'linkdin-icon'| escape  -%}
                                </a>
                            </li>
                        </ul>
                    </div>
                 {% else %}
                     <div class="social_icons" style="display:block">
                        <ul>
                             <li>
                                <a href="{{ settings.social_facebook_link }}">
                                     {%- render 'facebook icon'| escape  -%}
                                </a>
                            </li>
                            <li>
                                <a href="{{ settings.social_twitter_link }}">
                                     {%- render 'twitter-icon'| escape  -%}
                                </a>
                            </li>
                            <li>
                               <a href="{{ settings.social_youtube_link }}">
                                     {%- render 'youtube-icon'| escape  -%}
                                </a>
                            </li>
                            <li>
                                <a href="{{ settings.social_linkedin_link }}">
                                   {%- render 'linkdin-icon'| escape  -%}
                                </a>
                            </li>
                        </ul>
                    </div>
                {% endif %}
               </div>
                    {% endif %}     
                {% endfor %}                        

                  <div class="footer_center">
                  {% for block in section.blocks %}
                     {% if  block.type == "menu" %}
                             
                            {%- for link in block.settings.menu_link_center.links -%}
                              {%- if link.links != blank -%}
                              <div class="links f_list col">
                              {% if block.settings.m_head != blank %}
                                  <h6>
                                    {{ block.settings.m_head }}
                                  </h6>
                              {% else %}
                                  <h6>
                                    {{ link.title | escape }}
                                  </h6>
                              {% endif %}
                                  <ul>
                                   {%- for childlink in link.links -%}
                                     <li>
                                       {%- if childlink.links == blank -%}
                                         <a href="#">
                                           {{ childlink.title | escape }}
                                         </a>
                                       {% endif %}
                                     </li>
                                   {% endfor %}
                                  </ul>
                              </div>
                              {% endif %}
                            {% endfor %}
                    {% endif %}
                    {% endfor %}  
                     </div>   

                    {% for block in section.blocks %}
                    {% if  block.type == "newsletter" %}
                         <div class="footer_last">
                                <div class="links t_list col col">
                                      <h6>
                                         {{ block.settings.n_head }}
                                      </h6>
                                      <p>{{ block.settings.n_sub }}</p>
                                      <form action="#">
                                          <input type="text" placeholder="Email Address">
                                          <button type="submit" class="btn">
                                              <svg width="11" height="19" viewBox="0 0 11 19" fill="none"
                                                  xmlns="http://www.w3.org/2000/svg">
                                                  <path d="M2 17L8.72609 9.5L2 2" stroke="white" stroke-width="3" />
                                              </svg>
              
                                          </button>
                                      </form>
                                  </div>
                  
                                 <div class="fr_bottom col4 col">
                                               <div class="links six_list">
                                              
