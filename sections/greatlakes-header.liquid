 <header>
   {% if section.settings.anbar %}

      {% if section.settings.antext != blank %}
        
        <div class="anbar color-{{ section.settings.color_scheme }}" style="background:{{ section.settings.color_scheme }}"> 
          <p>{{ section.settings.antext }}</p>
          {% comment %}
            <p>Free shipping <span>for orders $24.95 and up! COVID-19 Status:</span> Orders are shipping on time!</p>
          {% endcomment %}
        </div>
     
     
      {% endif %}
   {% endif %}
        <div class="h_main">
            <div class="container">
                <div class="h_primery">
                    <div class="logo hb-{{ section.settings.logo_position}}" >
                  
                        <figure>
                            <a href="#">
                                <img src="{{ section.settings.logo  | img_url: 'original master' }}" alt="" style="max-width:{{ section.settings.logo_size}}px">
                            </a>
                        </figure>
                    
                    </div>
                  
                  <ul class="h_center d-sm-none d-flex hb-{{ section.settings.logo_position}}" style="margin-bottom:{{ section.settings.m_bottom}}px;">
                      {%- for link in section.settings.menu.links -%}
                        
                        <li class="dropdown_one" style="padding-top:{{ section.settings.p_top}}px;padding-bottom:{{ section.settings.p_bottom}}px;">
                            {%- if link.links != blank -%}
                            <a href="{{ link.url }}">
                                {{ link.title | escape }}
                                <span class="f_svg">
                                    <svg width="10" height="7" viewBox="0 0 10 7" fill="none" xmlns="http://www.w3.org/2000/svg">
                                        <path d="M8.75 0.5L10 1.68095L5 6.21429L-5.16211e-08 1.68095L1.25 0.5L5 3.92857L8.75 0.5Z"
                                            fill="white" />
                                    </svg>
                                </span>
                                <span class="s_svg d-none">
                    
                                    <svg width="10" height="6" viewBox="0 0 10 6" fill="none" xmlns="http://www.w3.org/2000/svg">
                                        <path
                                            d="M8.75 5.71429L10 4.53334L5 8.80163e-06L-5.16211e-08 4.53334L1.25 5.71429L5 2.28572L8.75 5.71429Z"
                                            fill="#5A5A5A" />
                                    </svg>
                    
                                </span>
                            </a>

                          {% assign gchildlink = false %}
                          {% for clinks in link.links  %}
                              {%- if clinks.links != blank -%}
                              {% assign gchildlink = true %}
                                {% endif %}
                           {% endfor %}
                              
                          {% if gchildlink %}
                            
                           <div class="mega_menu">
                                <div class="container">
                              
                                    <ul class="{% if link.title == "Products" %}mo_main {% endif %} {% if link.title == "Explore" %}ms_main {% endif %} {% if link.title == "B2B" %}mt_main {% endif %}">
                                        {%- for childlink in link.links -%}
                                      
                                        <li class="{% if link.title == "Products" %}col5 col{% endif %}{% if link.title == "Explore" %} {% endif %}{% if link.title == "B2B" %}col4 col{% endif %}">
                                          
                                          {%- for block in section.blocks -%} 
                                          {% assign match_Link = block.settings.title | handle %}
                                          {% assign child_Link_handle = childlink.title | handle %}
                                          {% if child_Link_handle == match_Link %}
                                          {{ block.settings.mega_svg }}
                                            {% endif %}
                                        {% endfor %}
                                        
                                          
                                            {%- if childlink.links != blank -%}
                                               <ul> 
                                                <h6>
                                                    <a href="{{ childlink.url }}">
                                                        {{ childlink.title | escape }}
                                                    </a>
                                                </h6>
                                           
                                                {%- for grandchildlink in childlink.links -%}
                                                 <li>
                                                    <a href="{{ grandchildlink.url  }}">
                                                        {{ grandchildlink.title | escape }}
                                                    </a>
                                                </li>
                                                {%- endfor -%}
                                            </ul>
                                              
                                            {% else %}
                                              
                                            {% for block in section.blocks %}
                                              {% if block.type == "text" %}
                                                {% assign match_head = block.settings.title_sec %}
                                                {% assign child_head = childlink.title   %}
                                                  <ul> 
                                                  <li>
                                              <h3>
                                                  <a href="#">
                                                         {{ childlink.title | escape }}
                                                  </a>
                                              </h3>
                                                 {% if  match_head == child_head %}
                                              {{ block.settings.mega_para }}
                                                {% endif %}
                                                 </li>
                                                 </ul>
                                              
                                              {% endif %}

                                              {% endfor %}
                                              

                                            {%- endif -%}
                                        </li>
                                          
                                        {%- endfor -%}
                                    </ul>
                                 
                                </div>
                            </div>

                             {% else %}
                              <div class="dropdown_menu">
                                <ul class="normal_dd">
                                   {%- for childlink in link.links -%}
                                     <li>
                                       <h3>
                                        <a href="{{ childlink.url  }}">
                                            {{ childlink.title | escape }}
                                        </a>
                                       </h3>
                                    </li>
                                    {% endfor %}
                                </ul>
                                  </div>           
                            {%- endif -%}

 
                            {% else %}
                            
                            <a href="#">
                                {{ link.title | escape }}
                            </a>
                      
                            {%- endif -%}
                        </li>

